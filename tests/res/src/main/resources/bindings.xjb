<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings jaxb:version="3.0"
	xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:inh="http://jvnet.org/basicjaxb/xjc/inheritance"
	jaxb:extensionBindingPrefixes="xjc inh"
>

	<jaxb:bindings schemaLocation="maven:org.patrodyne.jvnet:hisrc-higherjaxb-maven-plugin-tests-po:jar::!/purchaseorder.xsd" node="/xs:schema">

		<!--jaxb:schemaBindings>
			<jaxb:package name="org.jvnet.higherjaxb.mojo.tests.po"/>
		</jaxb:schemaBindings-->		

		<!-- See https://github.com/eclipse-ee4j/jaxb-ri/pull/1751 -->
		<jaxb:bindings node="xs:element[@name='comment']">
			<inh:annotates annotation="java.lang.SuppressWarnings">
				<inh:elements name="value" type="java.lang.String">rawtypes unchecked serial</inh:elements>
			</inh:annotates>
		</jaxb:bindings>

		<!-- See https://github.com/eclipse-ee4j/jaxb-ri/pull/1751 -->
		<jaxb:bindings node="xs:element[@name='purchaseOrder']">
			<inh:annotates annotation="java.lang.SuppressWarnings">
				<inh:elements name="value" type="java.lang.String">rawtypes unchecked serial</inh:elements>
			</inh:annotates>
		</jaxb:bindings>

	</jaxb:bindings>

</jaxb:bindings>
